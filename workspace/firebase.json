
{
  "emulators": {
    "dataconnect": {
      "dataDir": "app/dataconnect/.dataconnect/pgliteData"
    }
  },
  "dataconnect": {
    "source": "app/dataconnect"
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "app/firestore.rules",
    "indexes": "app/firestore.indexes.json"
  },
  "apphosting": {
    "backendId": "mx-backend",
    "rootDir": "app",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "functions"
    ]
  }
}
